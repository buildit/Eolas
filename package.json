{
  "name": "eolas",
  "version": "1.0.0",
  "description": "REST Services in support of the Buildit MI dashboard tools",
  "main": "index.js",
  "scripts": {
    "package": "gulp package",
    "lint": "echo '=> linting' && eslint api_doc/ routes/ services/ test/ test_accept/ *.js",
    "test": "echo '=> testing' && mocha test -R spec",
    "security": "echo '=> nsp testing' && nsp check --output summary",
    "accept": "echo '=> accept' && mocha test_accept -R spec",
    "validate": "echo '=> validating Eolas' && npm run lint && npm run test && npm run security"
  },
  "pre-commit": [
    "validate"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/digitalrigbitbucketteam/eolas.git"
  },
  "author": "buildit",
  "license": "MIT",
  "devDependencies": {
    "chakram": "^1.4.0",
    "del": "^2.2.1",
    "eslint": "^3.3.1",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-template": "^4.0.0",
    "mocha": "^3.0.2",
    "node-mocks-http": "^1.5.2",
    "nsp": "^2.6.1",
    "should": "^9.0.2"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "co": "^4.6.0",
    "config": "^1.21.0",
    "express": "^4.14.0",
    "log4js": "^0.6.37",
    "method-override": "^2.3.6",
    "mongodb": "^2.2.4",
    "response-time": "^2.3.1",
    "try": "^0.13.3"
  }
}
